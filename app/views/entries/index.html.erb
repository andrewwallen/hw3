<h1><%= @place["name"]%> </h1>

<div class="row">
   <% for entry in @entries %>
     <div class="col-3">
      <h4><%= entry["title"] %></h4>
      <p><%= entry["occurred_on"] %></p>
      <p><%= entry["description"] %></p>
      <a href="/places/<%= @place["id"]%>/entries/<%=entry["id"]%>/edit">Edit Post</a>
    </div>
  <% end %>
  </div>

<div>
<p><a href="/places/<%= @place["id"] %>/entries/new">New Post</a></p>
</div>
<p><a href="/places">Back to Places</a></p>